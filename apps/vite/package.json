{
  "name": "@callsign/vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3001",
    "build": "vite build",
    "preview": "vite preview --port 3001",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract 'src/**/*.tsx' --format crowdin --throws --out-file messages.json",
    "intl:compile": "formatjs compile messages.json --format crowdin --out-file src/messages/en.json",
    "intl:clean": "rm messages.json",
    "lint": "eslint --max-warnings 0 --ignore-path ../../.gitignore .",
    "lint:fix": "pnpm run lint -- --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@callsign/ui": "workspace:*",
    "@formatjs/intl-localematcher": "^0.2.24",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@sentry/react": "^6.19.1",
    "@types/styled-components": "^5.1.24",
    "axios": "^0.26.1",
    "jotai": "^1.6.1",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.24.7",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.2.2",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@callsign/eslint-config": "workspace:*",
    "@callsign/ts-config": "workspace:*",
    "@formatjs/cli": "^4.8.2",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.5",
    "@vitejs/plugin-react": "^1.2.0",
    "@vitest/ui": "^0.7.7",
    "c8": "^7.11.0",
    "jsdom": "^19.0.0",
    "msw": "^0.39.2",
    "vite": "^2.8.6",
    "vite-plugin-webfont-dl": "^2.0.0",
    "vitest": "^0.7.7"
  }
}
